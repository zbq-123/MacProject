/*标题刷新按钮*/
.title-refresh-icon{
    font-size: 28px!important;
    line-height: 24px!important;
}

/*顶部导航栏*/
.mui-bar .mui-pull-left .mui-icon {
    padding-right: 5px;
    font-size: 28px;
}
.mui-bar .mui-btn {
    font-weight: normal;
    font-size: 17px;
}
.mui-bar .mui-btn-link {
    top: 1px;
}
.cancel-a{
    padding-top: 10px;
    padding-bottom: 10px;
    z-index: 20;
}
.cancel-icon{
    height: 24px;
    width: 24px;
}
.tab-img{
    height: 24px;
    width: 24px;
}

/* 搜索*/
.search-box{
    display: flex;
    align-content: center;
    padding: 5px 10px;
    border-bottom: 1px solid#007aff;
}
.search-box input{
    width: 100%;
    border-radius: 0px;
    border-bottom-left-radius: 15px;
    border-top-left-radius: 15px;
}
.search-box button{
    width: 72px!important;
    height: 34px!important;
    padding: 0px!important;
    border-radius: 0px;
    border-bottom-right-radius: 15px;
    border-top-right-radius: 15px;
}

/* 筛选*/
.sifg-box{
    border-bottom: 1px solid#007aff;
    padding-bottom: 5px;
}
.sift-btn-box{
    margin-top: 5px;
    display: flex;
    align-content: center;
    border: 1px solid #dfdfdf;
    border-radius: 5px;
    height: 30px;
    width: 100%;
    position: relative;
}
.sift-btn-box input{
    font-size: 12px!important;
    letter-spacing: 1px;
    border: 0px;
    padding: 0px 5px;
    margin-left: 2px;
    height: 28px;
    color: #9a9a9a;
    margin-bottom: 0px!important;
}
.sift-btn-box span{
    width: 0;
    height: 0;
    border-top: 8px solid #007aff;
    border-right: 8px solid transparent;
    border-left: 8px solid transparent;
    margin: auto;
    margin-right: 5px;
}
