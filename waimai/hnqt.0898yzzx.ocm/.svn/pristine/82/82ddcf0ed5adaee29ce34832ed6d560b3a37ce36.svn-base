.mui-bar-nav {
    top: 0;
    -webkit-box-shadow: 0 0px 0px #fff;
    box-shadow: 0 0px 0px #fff;
}
.mui-bar {
    position: fixed;
    z-index: 10;
    right: 0;
    left: 0;
    height: 44px;
    padding-right: 10px;
    padding-left: 10px;
    border-bottom: 0;
    background-color: #ffffff;
    -webkit-box-shadow: 0 0 0px rgba(0,0,0,.85);
    box-shadow: 0 0 0px rgba(0,0,0,.85);
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}

.mui-content{
    background-color: rgba(255,255,255,0);
}

.mui-title {
    font-size: 18px;
    font-weight: bold;
    line-height: 44px;
    position: absolute;
    display: block;
    width: 100%;
    margin: 0 -10px;
    padding: 0;
    text-align: center;
    white-space: nowrap;
    color: #000;
}

/*弹出提示窗*/
.mui-popup-title {
    font-size: 14px;
    font-weight: 400;
    text-align: center;
}

.mui-popup-title+.mui-popup-text {
    font-family: inherit;
    font-size: 16px;
    margin: 5px 0 0;
}

.mui-popup-button {
    font-size: 14px;
    line-height: 44px;
    position: relative;
    display: block;
    overflow: hidden;
    box-sizing: border-box;
    width: 100%;
    height: 44px;
    padding: 0 5px;
    cursor: pointer;
    text-align: center;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #42CA47;
    background: rgba(255,255,255,.95);
    -webkit-box-flex: 1;
}

.mui-popup-button.mui-popup-button-bold {
    font-weight: 400;
}

/*自动消失弹出 toast*/
.mui-toast-container {
    width: 70%!important;
    line-height: 14px;
    position: fixed;
    z-index: 9999;
    bottom: 50px;
    left: 50%;
    -webkit-transition: opacity .3s;
    transition: opacity .3s;
    -webkit-transform: translate(-50%,0);
    transform: translate(-50%,0);
    opacity: 0;
    box-shadow: 0 15px 15px -15px rgba(66,202,71,1);
}

.mui-toast-container.mui-active {
    opacity: .9;
}

.mui-toast-message {
    font-size: 14px;
    padding: 15px;
    text-align: center;
    color: #fff;
    border-radius: 4px;
    background-color: #42CA47;
}