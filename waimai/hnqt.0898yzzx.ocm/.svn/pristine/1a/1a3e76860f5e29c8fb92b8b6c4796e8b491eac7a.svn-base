html,body{
    background: #fcfcfd;
}

/*订单列表*/
.mui-content>.mui-table-view:first-child {
    margin-top: 0px;
}

.mui-table-view {
    position: relative;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    list-style: none;
    background-color: rgba(255,255,255,0);
}

.mui-table-view:before {
    height: 0px;
}
.mui-table-view:after {
    height: 0px;
}

.mui-table-view-cell {
    position: relative;
    overflow: hidden;
    padding: 0px;
    -webkit-touch-callout: none;
}

.mui-table-view-cell>.mui-slider-handle {
    position: relative;
     background-color: rgba(255,255,255,0);
}

.mui-table-view-cell:after {
    height: 0px;
}

.mui-table-view-cell:first-child{
    margin-top: 8px;
}

.mui-table-view-cell:last-child{
    margin-bottom: 7px;
}

.mui-btn-danger, .mui-btn-negative, .mui-btn-red {
    color: #fff;
    border: 1px solid #eb3c1c;
    background-color: #eb3c1c;
}

.mui-table-view-cell>.mui-slider-left>.mui-btn, .mui-table-view-cell>.mui-slider-right>.mui-btn {
    position: relative;
    left: 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding: 0 30px;
    color: #fff;
    border: 0;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    margin: 7px 0 8px 0;
    border-radius: 8px;
}


.order-panel {
    margin: 7px 15px 8px 15px;
    background: #ffffff;
    border-radius: 8px;
    padding: 0 12px 12px 12px;
    box-shadow:0 0 4px 4px #f9f9fa;
}

.order-panel .top-info{
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 38px;
    border-bottom: 1px solid #f6f6f6;
}

.order-panel .top-info .store-name{
    font-size: 16px;
    color: #333333;
    font-weight: bold;
}

.order-panel .top-info .store-name span{
    font-size: 18px;
    color: #999999;
    padding-left: 5px;
}

.order-panel .top-info .order-status{
    font-size: 14px;
    line-height: 14px;
}

.order-panel .center-info{
    display: flex;
    justify-items: start;
    margin-top: 12px;
}

.order-panel .center-info .goods-img{
    width: 74px;
    height: 74px;
    border-radius: 4px;
    flex-shrink: 0;
}

.order-panel .center-info .goods{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 12px;
    flex: 1;
}

.order-panel .center-info .goods .goods-name{
    font-size: 14px;
    line-height: 14px;
    font-weight: bold;
    color: #333;
    margin-bottom: 8px;
}

.order-panel .center-info .goods .goods-time{
    font-size: 12px;
    line-height: 12px;
    color: #999;
}

.order-panel .center-info .order-count{
    text-align: right;
    font-weight: bold;
    display: inline-block;
    vertical-align: bottom;
}

.order-panel .center-info .order-count .count-all{
    font-size: 12px;
    line-height: 12px;
    color: #333333;
}

.order-panel .center-info .order-count .count-tip{
    font-size: 12px;
    line-height: 12px;
    color: #333333;
}

.order-panel .center-info .order-count .count-price{
    font-size: 14px;
    line-height: 14px;
    color: #eb3c1c;
}

.order-panel .bottom-btn{
    text-align: right;
    margin-top: 15px;
}

.order-panel .bottom-btn .order-btn-normal{
    font-size: 12px;
    line-height: 20px !important;
    text-align: center;
    height: 18px;
    color: #333;
    border: 1px solid #eee;
    border-radius: 4px;
    padding: 4px 10px;
    margin-left: 8px;
}

.add-more-div{
    text-align: center;
    color: #666;
    font-size: 12px;
    line-height: 12px;
    margin: 20px 0 30px 0;
}


.mui-pull-right img{
    height: 20px;
    width: 20px;
}