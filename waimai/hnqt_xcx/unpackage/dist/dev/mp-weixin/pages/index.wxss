page {
  background: linear-gradient(to right, #fedb74, #ffbf80);
}
.u-loadmore {
  padding-bottom: 24rpx;
}
.bg-color {
  border-top-left-radius: 60rpx;
  border-top-right-radius: 60rpx;
  background: linear-gradient(to bottom, #fefefd, #eeebe1);
}
.bg-color .model {
  display: flex;
  align-items: center;
  padding-bottom: 46rpx;
  justify-content: space-between;
  flex-wrap: wrap;
}
.bg-color .model view {
  width: 20%;
  display: flex;
  flex-flow: column;
  align-items: center;
  margin-top: 46rpx;
}
.bg-color .model text {
  margin-top: 8rpx;
  font-size: 23rpx;
  display: inline-block;
}
.bg-color .model image {
  width: 80rpx;
  height: 80rpx;
}
.bg-color .canteen {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.bg-color ._h1 {
  font-weight: bold;
  margin: 25rpx 38rpx;
  display: inline-block;
  position: relative;
  padding-left: 34rpx;
  font-size: 26rpx;
  color: #101010;
}
.bg-color ._h1::before {
  content: '';
  position: absolute;
  left: 0;
  width: 30rpx;
  height: 30rpx;
  background: #000000;
  border-radius: 50%;
}
.u-navbar--fixed {
  background: linear-gradient(to right, #fedb74, #ffbf80);
}
.u-popup__content__close {
  bottom: -26px;
  left: 45%;
}
.u-popup__content__close text {
  color: white !important;
}
.coupon {
  width: 476rpx;
  color: white;
  display: flex;
  flex-flow: column;
  position: relative;
  margin-bottom: 10rpx;
}
.coupon image {
  width: 100%;
}
.coupon .honbao {
  position: absolute;
  top: 25rpx;
  left: 20rpx;
  display: flex;
  flex-flow: column;
}
.coupon .honbao text {
  font-size: 38rpx;
  color: #FCEDD0;
  margin-bottom: 10rpx;
}
.coupon .honbao view {
  font-size: 18rpx;
  color: #FCEDD0;
  margin-bottom: 10rpx;
}
.coupon > text {
  position: absolute;
  color: #DF0018;
  font-size: 145rpx;
  top: 50%;
  right: 10%;
  -webkit-transform: translate(0, -50%);
          transform: translate(0, -50%);
}
.u-scroll-list {
  margin-left: 38rpx;
  padding-bottom: 0 !important;
}
.todayFood {
  margin: 8rpx;
  box-shadow: 0rpx 0rpx 6rpx 0rpx rgba(0, 0, 0, 0.23);
  border-radius: 10rpx;
}
.tfoot {
  padding: 6rpx;
  background: white;
  display: flex;
  flex-flow: column;
}
.tfoot .goodsname {
  font-size: 25rpx;
  font-weight: 400;
  color: #292929;
}
.tfoot .goodsdetail {
  display: flex;
  justify-content: space-between;
}
.tfoot .goodsdetail .goodsprice {
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
}
.tfoot .goodsdetail .goodsprice text:first-child {
  font-size: 30rpx;
  font-weight: bold;
  color: #CB0200;
  margin-right: 4rpx;
  font-family: '黑体';
}
.tfoot .goodsdetail .goodsprice text:last-child {
  font-size: 25rpx;
  font-weight: 500;
  text-decoration: line-through;
  color: #A2A2A2;
  font-family: '黑体';
}
.tfoot .goodsdetail .button {
  width: 66rpx;
  height: 32rpx;
  line-height: 32rpx;
  background: #CB0200;
  border-radius: 6rpx;
  font-size: 16rpx;
  font-weight: 400;
  color: #FFFFFF;
  text-align: center;
}
.list {
  display: flex;
  flex-flow: column;
  background: linear-gradient(to right, burlywood, orangered);
  color: white;
  padding: 40rpx;
  font-size: 25rpx;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.2);
}
.dinwei {
  background: linear-gradient(to right, #fedb74, #ffbf80);
  font-size: 26rpx;
  font-weight: bold;
  color: #101010;
  padding: 0 15rpx 15rpx 15rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.dinwei text {
  font-size: 29rpx;
}
.dinwei .temperature {
  font-size: 26rpx;
  font-weight: bold;
  color: #101010;
}
.search {
  padding: 0 15rpx 15rpx 15rpx;
  background: linear-gradient(to right, #fedb74, #ffbf80);
}
.img2 {
  width: 100rpx;
  height: 100rpx;
}
.on {
  width: 60rpx;
  height: 28rpx;
  background: #FFEF9D;
  border: 2rpx solid #FF8C00;
  border-radius: 6rpx;
  font-size: 14rpx;
  font-family: Source Han Sans SC;
  font-weight: bold;
  color: #FF8C00;
  line-height: 28rpx;
  text-align: center;
}
.no {
  width: 60rpx;
  height: 28rpx;
  border: 2rpx solid #9B9B9B;
  border-radius: 6rpx;
  font-size: 14rpx;
  font-family: Source Han Sans SC;
  font-weight: bold;
  color: #9B9B9B;
  line-height: 28rpx;
  text-align: center;
}
._ul {
  display: flex;
  align-items: center;
  justify-content: space-around;
}
._li {
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 28rpx;
}
.vie1 {
  background: white;
  border-radius: 10rpx;
  overflow: hidden;
  width: 43.5%;
  margin-bottom: 24rpx;
  margin-left: 38rpx;
  box-shadow: 0 0 16rpx 0 rgba(0, 0, 0, 0.15);
}
.vie1 .img3 {
  width: 100%;
  height: 190rpx;
}
.vie1:nth-child(2n) {
  margin-left: 0;
  margin-right: 38rpx;
}
.vie2 {
  display: flex;
  flex-direction: column;
  padding: 16rpx;
}
.vie2 > label {
  margin-top: 14rpx;
  font-size: 20rpx;
  color: #ADADAD;
  display: flex;
  align-items: center;
}
.vie22 {
  margin-bottom: 130rpx;
}
.h2 {
  font-size: 30rpx;
  font-weight: bold;
  color: #101010;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.h2 ._h2 {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  flex: 1;
}
swiper {
  height: 238rpx;
  overflow: hidden;
  margin: auto;
  border-radius: 30rpx;
  margin: 0 40rpx;
}
swiper-item {
  height: 240rpx !important;
}
.swiper-item {
  width: 100%;
  height: 100%;
}
image {
  width: 100%;
  height: 100%;
  text-align: center;
  border-radius: 10rpx;
}

